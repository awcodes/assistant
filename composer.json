{
  "name": "awcodes/assistant",
  "description": "A collection of helpers for use in Laravel projects.",
  "keywords": [
    "laravel",
    "helpers",
    "utilities"
  ],
  "homepage": "https://github.com/awcodes/assistant",
  "license": "MIT",
  "authors": [
    {
      "name": "awcodes",
      "email": "awcodes1@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-intl": "*",
    "spatie/valuestore": "^1.3"
  },
  "require-dev": {
    "laravel/laravel": "^10.0",
    "laravel/pint": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Awcodes\\Assistant\\": "src"
    }
  },
  "scripts": {
    "pint": "vendor/bin/pint"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
